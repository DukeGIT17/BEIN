{
  "openapi": "3.0.1",
  "info": {
    "title": "BEIN API",
    "version": "1.0"
  },
  "paths": {
    "/api.bein.com/Account/Register": {
      "post": {
        "tags": [
          "Account"
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/RegistrationModel"
              }
            },
            "text/json": {
              "schema": {
                "$ref": "#/components/schemas/RegistrationModel"
              }
            },
            "application/*+json": {
              "schema": {
                "$ref": "#/components/schemas/RegistrationModel"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Success"
          }
        }
      }
    },
    "/api.bein.com/Account/SignIn": {
      "post": {
        "tags": [
          "Account"
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/SignInModel"
              }
            },
            "text/json": {
              "schema": {
                "$ref": "#/components/schemas/SignInModel"
              }
            },
            "application/*+json": {
              "schema": {
                "$ref": "#/components/schemas/SignInModel"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Success"
          }
        }
      }
    },
    "/api.bein.com/Account/SignOut": {
      "post": {
        "tags": [
          "Account"
        ],
        "responses": {
          "200": {
            "description": "Success"
          }
        }
      }
    },
    "/api.bein.com/AdminFunctions/AddSector": {
      "post": {
        "tags": [
          "AdminFunctions"
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/Sector"
              }
            },
            "text/json": {
              "schema": {
                "$ref": "#/components/schemas/Sector"
              }
            },
            "application/*+json": {
              "schema": {
                "$ref": "#/components/schemas/Sector"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Success"
          }
        }
      }
    },
    "/api.bein.com/AdminFunctions/AddSoftwareProduct": {
      "post": {
        "tags": [
          "AdminFunctions"
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/SoftwareProduct"
              }
            },
            "text/json": {
              "schema": {
                "$ref": "#/components/schemas/SoftwareProduct"
              }
            },
            "application/*+json": {
              "schema": {
                "$ref": "#/components/schemas/SoftwareProduct"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Success"
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "Feature": {
        "required": [
          "description",
          "title"
        ],
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "nullable": true
          },
          "title": {
            "maxLength": 200,
            "minLength": 3,
            "type": "string"
          },
          "description": {
            "maxLength": 3000,
            "minLength": 3,
            "type": "string"
          },
          "softwareProduct": {
            "$ref": "#/components/schemas/SoftwareProduct"
          },
          "softwareProductId": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "RegistrationModel": {
        "required": [
          "email",
          "name",
          "password",
          "surname"
        ],
        "type": "object",
        "properties": {
          "name": {
            "maxLength": 50,
            "minLength": 3,
            "type": "string"
          },
          "surname": {
            "maxLength": 50,
            "minLength": 3,
            "type": "string"
          },
          "email": {
            "minLength": 1,
            "type": "string",
            "format": "email"
          },
          "password": {
            "minLength": 1,
            "type": "string",
            "format": "password"
          }
        },
        "additionalProperties": false
      },
      "Sector": {
        "required": [
          "description",
          "title"
        ],
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "nullable": true
          },
          "title": {
            "maxLength": 250,
            "minLength": 0,
            "type": "string"
          },
          "description": {
            "maxLength": 5000,
            "minLength": 0,
            "type": "string"
          },
          "products": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/SectorProduct"
            },
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "SectorProduct": {
        "type": "object",
        "properties": {
          "productId": {
            "type": "string",
            "nullable": true
          },
          "productName": {
            "type": "string",
            "nullable": true
          },
          "product": {
            "$ref": "#/components/schemas/SoftwareProduct"
          },
          "sectorId": {
            "type": "string",
            "nullable": true
          },
          "sectorTitle": {
            "type": "string",
            "nullable": true
          },
          "sector": {
            "$ref": "#/components/schemas/Sector"
          }
        },
        "additionalProperties": false
      },
      "SignInModel": {
        "required": [
          "email",
          "password"
        ],
        "type": "object",
        "properties": {
          "email": {
            "minLength": 1,
            "type": "string",
            "format": "email"
          },
          "password": {
            "minLength": 1,
            "type": "string",
            "format": "password"
          }
        },
        "additionalProperties": false
      },
      "SoftwareProduct": {
        "required": [
          "description",
          "name",
          "vendor"
        ],
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "nullable": true
          },
          "name": {
            "maxLength": 100,
            "minLength": 0,
            "type": "string"
          },
          "description": {
            "maxLength": 10000,
            "minLength": 0,
            "type": "string"
          },
          "vendor": {
            "maxLength": 100,
            "minLength": 0,
            "type": "string"
          },
          "rating": {
            "type": "integer",
            "format": "int32"
          },
          "review": {
            "maxLength": 5000,
            "minLength": 0,
            "type": "string",
            "nullable": true
          },
          "sectors": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/SectorProduct"
            },
            "nullable": true
          },
          "projectStage": {
            "type": "string",
            "nullable": true
          },
          "professions": {
            "type": "array",
            "items": {
              "type": "string"
            },
            "nullable": true
          },
          "features": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/Feature"
            },
            "nullable": true
          },
          "visits": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/Visit"
            },
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "User": {
        "required": [
          "email",
          "name",
          "profession",
          "surname"
        ],
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "nullable": true
          },
          "name": {
            "maxLength": 50,
            "minLength": 3,
            "type": "string"
          },
          "surname": {
            "maxLength": 50,
            "minLength": 3,
            "type": "string"
          },
          "email": {
            "minLength": 1,
            "type": "string",
            "format": "email"
          },
          "phoneNumber": {
            "type": "string",
            "format": "tel",
            "nullable": true
          },
          "profession": {
            "minLength": 1,
            "type": "string"
          },
          "yearsOfExperience": {
            "type": "integer",
            "format": "int32"
          },
          "visits": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/Visit"
            },
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "Visit": {
        "type": "object",
        "properties": {
          "userId": {
            "type": "string",
            "nullable": true
          },
          "name": {
            "type": "string",
            "nullable": true
          },
          "surname": {
            "type": "string",
            "nullable": true
          },
          "user": {
            "$ref": "#/components/schemas/User"
          },
          "softwareProductId": {
            "type": "string",
            "nullable": true
          },
          "productName": {
            "type": "string",
            "nullable": true
          },
          "product": {
            "$ref": "#/components/schemas/SoftwareProduct"
          }
        },
        "additionalProperties": false
      }
    }
  }
}