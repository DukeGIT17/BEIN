  "components": {
    "schemas": {
      "Feature": {
        "required": [
          "description",
          "title"
        ],
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "nullable": true,
            "readOnly": true
          },
          "title": {
            "maxLength": 200,
            "minLength": 3,
            "type": "string"
          },
          "description": {
            "maxLength": 3000,
            "minLength": 3,
            "type": "string"
          },
          "softwareProductId": {
            "type": "string",
            "nullable": true
          },
          "softwareProduct": {
            "$ref": "#/components/schemas/SoftwareProduct"
          }
        },
        "additionalProperties": false
      },
      "Rating": {
        "required": [
          "ratingValue"
        ],
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "nullable": true,
            "readOnly": true
          },
          "ratingValue": {
            "maximum": 5,
            "minimum": 1,
            "minLength": 1,
            "type": "string"
          },
          "softwareId": {
            "type": "string",
            "nullable": true
          },
          "userId": {
            "type": "string",
            "nullable": true
          },
          "software": {
            "$ref": "#/components/schemas/SoftwareProduct"
          },
          "review": {
            "$ref": "#/components/schemas/Review"
          },
          "user": {
            "$ref": "#/components/schemas/User"
          }
        },
        "additionalProperties": false
      },
      "RegistrationModel": {
        "required": [
          "email",
          "name",
          "password",
          "surname"
        ],
        "type": "object",
        "properties": {
          "name": {
            "maxLength": 50,
            "minLength": 3,
            "type": "string"
          },
          "surname": {
            "maxLength": 50,
            "minLength": 3,
            "type": "string"
          },
          "email": {
            "minLength": 1,
            "type": "string",
            "format": "email"
          },
          "password": {
            "minLength": 1,
            "type": "string",
            "format": "password"
          }
        },
        "additionalProperties": false
      },
      "Review": {
        "required": [
          "header",
          "reviewText",
          "reviewTime"
        ],
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "nullable": true,
            "readOnly": true
          },
          "header": {
            "maxLength": 70,
            "minLength": 3,
            "type": "string"
          },
          "reviewText": {
            "maxLength": 1200,
            "minLength": 50,
            "type": "string"
          },
          "reviewTime": {
            "type": "string",
            "format": "date-time"
          },
          "ratingId": {
            "type": "string",
            "nullable": true
          },
          "rating": {
            "$ref": "#/components/schemas/Rating"
          }
        },
        "additionalProperties": false
      },
      "Sector": {
        "required": [
          "description",
          "shortDescription",
          "title"
        ],
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "nullable": true,
            "readOnly": true
          },
          "title": {
            "maxLength": 50,
            "minLength": 0,
            "type": "string"
          },
          "shortDescription": {
            "maxLength": 110,
            "minLength": 0,
            "type": "string"
          },
          "description": {
            "maxLength": 700,
            "minLength": 0,
            "type": "string"
          },
          "products": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/SectorProduct"
            },
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "SectorProduct": {
        "type": "object",
        "properties": {
          "productId": {
            "type": "string",
            "nullable": true
          },
          "productName": {
            "type": "string",
            "nullable": true
          },
          "product": {
            "$ref": "#/components/schemas/SoftwareProduct"
          },
          "sectorId": {
            "type": "string",
            "nullable": true
          },
          "sectorTitle": {
            "type": "string",
            "nullable": true
          },
          "sector": {
            "$ref": "#/components/schemas/Sector"
          }
        },
        "additionalProperties": false
      },
      "SignInModel": {
        "required": [
          "email",
          "password"
        ],
        "type": "object",
        "properties": {
          "email": {
            "minLength": 1,
            "type": "string",
            "format": "email"
          },
          "password": {
            "minLength": 1,
            "type": "string",
            "format": "password"
          }
        },
        "additionalProperties": false
      },
      "SoftwareProduct": {
        "required": [
          "description",
          "name",
          "vendor"
        ],
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "nullable": true,
            "readOnly": true
          },
          "imageName": {
            "type": "string",
            "nullable": true
          },
          "imageBase64": {
            "type": "string",
            "nullable": true
          },
          "name": {
            "maxLength": 100,
            "minLength": 0,
            "type": "string"
          },
          "description": {
            "maxLength": 700,
            "minLength": 0,
            "type": "string"
          },
          "vendor": {
            "maxLength": 100,
            "minLength": 0,
            "type": "string"
          },
          "projectStage": {
            "type": "string",
            "nullable": true
          },
          "professions": {
            "type": "array",
            "items": {
              "type": "string"
            },
            "nullable": true
          },
          "sectors": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/SectorProduct"
            },
            "nullable": true
          },
          "ratings": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/Rating"
            },
            "nullable": true
          },
          "features": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/Feature"
            },
            "nullable": true
          },
          "visits": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/Visit"
            },
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "User": {
        "required": [
          "email",
          "name",
          "profession",
          "surname"
        ],
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "nullable": true,
            "readOnly": true
          },
          "name": {
            "maxLength": 50,
            "minLength": 3,
            "type": "string"
          },
          "surname": {
            "maxLength": 50,
            "minLength": 3,
            "type": "string"
          },
          "email": {
            "minLength": 1,
            "type": "string",
            "format": "email"
          },
          "phoneNumber": {
            "type": "string",
            "format": "tel",
            "nullable": true
          },
          "profession": {
            "minLength": 1,
            "type": "string"
          },
          "yearsOfExperience": {
            "type": "integer",
            "format": "int32"
          },
          "ratings": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/Rating"
            },
            "nullable": true
          },
          "visits": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/Visit"
            },
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "Visit": {
        "type": "object",
        "properties": {
          "userId": {
            "type": "string",
            "nullable": true,
            "readOnly": true
          },
          "name": {
            "type": "string",
            "nullable": true
          },
          "surname": {
            "type": "string",
            "nullable": true
          },
          "user": {
            "$ref": "#/components/schemas/User"
          },
          "softwareProductId": {
            "type": "string",
            "nullable": true,
            "readOnly": true
          },
          "productName": {
            "type": "string",
            "nullable": true
          },
          "product": {
            "$ref": "#/components/schemas/SoftwareProduct"
          }
        },
        "additionalProperties": false
      }
    }
  }
}